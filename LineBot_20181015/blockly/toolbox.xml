      <block type="text">
        <field name="TEXT"></field>
      </block>
    </value>   
    <value name="bot_userid">
      <block type="text">
        <field name="TEXT"></field>
      </block>
    </value>
    <value name="bot_msg">
      <block type="linebot_type">
        <value name="value_parameter1">
          <block type="variables_get">
            <field name="VAR">parameter1</field> 
          </block>
        </value>
        <value name="value_parameter2">
          <block type="variables_get">
            <field name="VAR">parameter2</field> 
          </block>
        </value> 
        <value name="value_parameter3">
          <block type="variables_get">
            <field name="VAR">parameter3</field> 
          </block>
        </value> 
        <value name="value_parameter4">
          <block type="variables_get">
            <field name="VAR">parameter4</field> 
          </block>
        </value>         
      </block>  
    </value>
  </block>
</category>
