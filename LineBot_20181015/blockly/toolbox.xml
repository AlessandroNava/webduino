<category id="catlinebot">
  <block type="linebot_notify">
    <value name="bot_token">
      <block type="text">
        <field name="TEXT"></field>
      </block>
    </value>   
    <value name="bot_userid">
      <block type="text">
        <field name="TEXT"></field>
      </block>
    </value>
    <value name="bot_msg">
      <block type="linebot_type">
        <value name="value_paramter1">
          <block type="variables_get">     
            <field name="VAR">paramter1</field>
          </block>
        </value>
        <value name="value_paramter2">
          <block type="variables_get">     
            <field name="VAR">paramter2</field>
          </block>
        </value> 
      </block>  
    </value>
  </block>
</category>
